---
import Layout from "../layouts/Layout.astro";
import ReactComponent from "../components/ReactComponent";

const data = await fetch("http://localhost:7100/manifest.json");
const manifest = await data.json();

const cssEntrypoint = manifest["src/MicroFrontend.css"]["file"];
---

<Layout title="Shell" cssEntrypoint={cssEntrypoint}>
  <main>
    <h1>Shell app</h1>
    <ReactComponent manifest={manifest}>
  </main>
</Layout>
